@model List<u21517208_HW04.Models.Gallery>
@{
    ViewBag.Title = "Gallery";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Gallery</h2>

<p> Welcome to the Gallery</p>
@*(string name, string location, int peoplefed)*@
<p>In this view you will see the different projects we have done </p>
<br />
<table>
    <tr>
        <th>Name</th>
        <th>Location</th>
        <th>Number of People Fed</th>
    </tr>
    @foreach (var data in Model)
    {
        <tr>
            <td>@data.Name</td>
            <td>@data.Location</td>
            <td>@data.PeopleFed</td>
        </tr>
    }
</table>